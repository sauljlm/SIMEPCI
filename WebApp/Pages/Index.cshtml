<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow fixed-top">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index"><img src="https://i.imgur.com/KNLtoSa.png" alt="Logo SIMEPCI" style="width:150px"/></a>
                <div class="navbar-collapse collapse justify-content-center">
                    <ul class="navbar-nav">
                        <li class="nav-item"><a href="#divInicio" class="nav-link px-2 fst-normal" style="color:#0353A4">Inicio</a></li>
                        <li class="nav-item"><a href="#divVentajas" class="nav-link px-2 fst-normal" style="color:#0353A4">Ventajas</a></li>
                        <li class="nav-item"><a href="#divCitas" class="nav-link px-2 fst-normal" style="color:#0353A4">Citas</a></li>
                        <li class="nav-item"><a href="#divEspecialidades" class="nav-link px-2 fst-normal" style="color:#0353A4">Especialidades</a></li>
                        <li class="nav-item"><a href="#divFaqs" class="nav-link px-2 fst-normal" style="color:#0353A4">FAQs</a></li>
                    </ul>
                </div>
                <div>
                    <a asp-area="" asp-page="/Login" class="me-1 border-0 bg-transparent fst-normal text-decoration-none" style="color:#0353A4">Inicia sesión</a>
                    <a asp-area="" asp-page="/RegistroUsuario" class="rounded-3 text-light border-0 p-2 text-decoration-none" style="background-color:#0353A4">Regístrate</a>
                </div>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
            </div>
        </nav> 
    </header>

    <!-- section 1 Jumbotron-->
    <div id="divInicio" class="jumbotron jumbotron-fluid mb-4 bg-white">
        <div class="vh-100 align-items-center justify-content-center" style="display: flex; justify-content: center;">
            <div class="col">
                <div class="d-flex flex-column justify-content-center align-items-center h-100">
                    <h1 class="display-2 fw-normal text-left" style="color:#0353A4; width:35rem;">Hospital SIMEPCI</h1>
                    <p class="display-6 text-left" style="color:dimgrey; width:35rem;">Ofreciendo servicios médicos de calidad para toda la comunidad</p>
                    <!--  <button class="btn btn-primary btn-lg" type="button">Botón de ejemplo</button> -->
                </div>
            </div>
            <div class="col">
                <div class="d-flex flex-column justify-content-center align-items-center" style="background-image: url('https://i.imgur.com/oYeosKe.png'); background-size: 90vh 75vh; height:100%; width:100%; position: relative; background-repeat: no-repeat; background-position: top 100% left 100%; overflow: hidden;">
                    <img src="https://i.imgur.com/lfxm6fO.png" style="width:60vh; height:85vh; position: absolute; bottom: 0; left: 50%; transform: translateX(-50%); overflow: hidden" alt="Alternate Text" />
                </div>
            </div>
        </div>
    </div>

